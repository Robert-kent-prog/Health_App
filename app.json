{
  "expo": {
    "name": "HealthApp",
    "slug": "HealthApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/applogo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#009387"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/applogo.png",
      "backgroundColor": "#009387"
    },

    "experiments": {
      "typedRoutes": true
    }
  }
}
